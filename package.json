{
  "name": "DockerDonkey",
  "version": "1.0.0",
  "description": "Webinterface zum Verwalten von Docker auf Ubuntu mit Datei-Uploads und Git-Update-Check.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"tsc --watch --preserveWatchOutput\"",
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "ejs": "^3.1.10",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "nodemon": "^3.1.7",
    "typescript": "^5.5.3"
  },
  "license": "MIT",
  "private": true
}
